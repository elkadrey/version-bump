{
    "name": "elkadrey/version-bump",
    "description": "Versions handles versioning in composer",
    "type": "composer-plugin",
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0",
        "symfony/process": "^6.0",
        "composer/composer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Elkadrey\\VersionBump\\": "src/"
        }
    },
    "extra": {
        "composer-plugin": {
            "class": "Elkadrey\\VersionBump\\VersionPlugin"
        }
    }
}
